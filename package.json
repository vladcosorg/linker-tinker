{
  "name": "link-and-tink",
  "version": "1.0.0",
  "description": "oclif example Hello World CLI",
  "author": "chetzof @chetzof",
  "bin": {
    "link-and-tink": "./bin/run",
    "lnt": "./bin/run"
  },
  "prettier": "chetzof-lint-config/prettier/.prettierrc.js",
  "homepage": "https://github.com/chetzof/link-and-tink",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "chetzof/link-and-tink",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@npmcli/arborist": "^6.2.0",
    "@oclif/core": "^2.0.7",
    "@oclif/plugin-autocomplete": "^1.4.2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-not-found": "^2.3.17",
    "@oclif/plugin-warn-if-update-available": "^2.0.23",
    "chalk": "4.1.2",
    "chetzof-lint-config": "file:../lint-config",
    "chokidar": "^3.5.3",
    "execa": "^5.1.1",
    "fs-extra": "^11.1.0",
    "fs-jetpack": "^5.1.0",
    "listr2": "^5.0.7",
    "log-symbols": "5.1.0",
    "node-notifier": "^10.0.1",
    "npm-packlist": "^7.0.4",
    "pidtree": "^0.6.0",
    "pm2": "^5.2.2",
    "tiny-typed-emitter": "^2.1.0",
    "tsconfig-paths": "^4.1.2",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@types/chai": "^4",
    "@types/chokidar": "^2.1.3",
    "@types/fs-extra": "^11.0.1",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.18",
    "@types/node-notifier": "^8.0.2",
    "@types/npm-packlist": "^7.0.0",
    "@types/npmcli__arborist": "^5.6.1",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "eslint-define-config": "^1.14.0",
    "husky": "^8.0.3",
    "lodash": "^4.17.21",
    "oclif": "^3",
    "semantic-release": "^20.1.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.2",
    "tslib": "^2.5.0",
    "type-fest": "^3.5.3",
    "typescript": "^4.9.4",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "0.28.3",
    "vitest-mock-process": "^1.0.4"
  },
  "oclif": {
    "bin": "linktink",
    "dirname": "linktink",
    "default": "sync",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-warn-if-update-available",
      "@oclif/plugin-not-found"
    ]
  },
  "scripts": {
    "build": "shx rm -rf dist; tsc; tsc-alias",
    "watch": "tsc -w",
    "lint": "npm run lint:tsc; npm run lint:eslint",
    "fix": "npm run fix:prettier . && npm run fix:eslint .",
    "fix:prettier": "prettier --write --ignore-unknown",
    "fix:eslint": "NODE_ENV=production  eslint --fix",
    "lint:eslint": "NODE_ENV=production  eslint .",
    "lint:tsc": "tsc --noEmit",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "vitest",
    "version": "oclif readme && git add README.md",
    "prepare": "husky install",
    "semantic-release": "semantic-release",
    "lint-staged:test": "vitest related --run",
    "pm2": "pm2"
  },
  "engines": {
    "node": ">=16"
  },
  "bugs": "https://github.com/chetzof/linktink/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts",
  "release": {
    "branches": [
      "main"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
